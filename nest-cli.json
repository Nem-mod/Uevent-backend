    "analytic": {
      "type": "application",
      "root": "apps/analytic",
      "entryFile": "main",
      "sourceRoot": "apps/analytic/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytic/tsconfig.app.json"
      }
