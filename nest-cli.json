    "email": {
      "type": "application",
      "root": "apps/email",
      "entryFile": "main",
      "sourceRoot": "apps/email/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email/tsconfig.app.json"
      }
    },
    "comments": {
      "type": "application",
      "root": "apps/comments",
      "entryFile": "main",
      "sourceRoot": "apps/comments/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comments/tsconfig.app.json"
      }
