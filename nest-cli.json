      "compilerOptions": {
        "tsConfigPath": "apps/event/tsconfig.app.json"
      }
    },
    "ticket": {
      "type": "application",
      "root": "apps/ticket",
      "entryFile": "main",
      "sourceRoot": "apps/ticket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ticket/tsconfig.app.json"
      }
    },
    "comment": {
      "type": "application",
      "root": "apps/comment",
      "entryFile": "main",
      "sourceRoot": "apps/comment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment/tsconfig.app.json"
      }
